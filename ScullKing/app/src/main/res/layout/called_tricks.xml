<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cortuga">


    <SeekBar
        android:id="@+id/seekBar_called_6"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toEndOf="@+id/textView_called_points_6"
        app:layout_constraintTop_toBottomOf="@+id/seekBar_called_5" />

    <TextView
        android:id="@+id/textView_called_progress_6"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="10dp"
        android:textColor="#F4F4F4"
