    <Button
        android:id="@+id/set_Tricks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="40dp"
        android:text="OK_called_tricks"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView_called_points_1"
        android:layout_width="125dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="100dp"
        android:textColor="#FAF6F6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <SeekBar
        android:id="@+id/seekBar_called_1"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="100dp"
        android:accessibilityLiveRegion="none"
        android:backgroundTint="#FFFFFF"
        android:backgroundTintMode="multiply"
        android:max="10"
        android:progress="2"
        android:scrollbarSize="4dp"
        android:scrollbarStyle="outsideOverlay"
        app:layout_constraintStart_toEndOf="@+id/textView_called_points_1"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_called_progress_1"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="10dp"
