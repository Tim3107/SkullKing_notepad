<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/scarymary"
    tools:context=".Activity_set_actual_tricks">

    <SeekBar
        android:id="@+id/seekBar7"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toEndOf="@+id/textView_actual_6"
        app:layout_constraintTop_toBottomOf="@+id/seekBar6" />

    <TextView
        android:id="@+id/textView_actual_progress_6"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_actual_progress_5" />

    <SeekBar
        android:id="@+id/seekBar6"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toEndOf="@+id/textView_actual_5"
        app:layout_constraintTop_toBottomOf="@+id/seekBar5" />

    <TextView
        android:id="@+id/textView_actual_progress_5"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_actual_progress_4" />

    <SeekBar
        android:id="@+id/seekBar5"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toEndOf="@+id/textView_actual_4"
        app:layout_constraintTop_toBottomOf="@+id/seekBar4" />

    <TextView
        android:id="@+id/textView_actual_progress_4"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:textColor="#FDFDFD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seekBar4" />

    <SeekBar
        android:id="@+id/seekBar4"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toEndOf="@+id/textView_actual_3"
        app:layout_constraintTop_toBottomOf="@+id/seekBar3" />

    <TextView
        android:id="@+id/textView_actual_progress_3"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_actual_progress_2" />

    <SeekBar
        android:id="@+id/seekBar3"
        style="@android:style/Widget.SeekBar"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="30dp"
        android:max="10"
        android:progress="3"
        app:layout_constraintStart_toEndOf="@+id/textView_actual_2"
        app:layout_constraintTop_toBottomOf="@+id/seekBar1" />

    <TextView
        android:id="@+id/textView_actual_progress_2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="15dp"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_actual_progress_1" />

    <TextView
        android:id="@+id/textView_actual_6"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        android:textColor="#F6F3F3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_actual_5" />

    <TextView
        android:id="@+id/textView_actual_5"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="30dp"
        android:textColor="#FFFEFE"
